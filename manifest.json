{
  "name": "Travis Report",
  "version" : "1.0.0",
  "manifest_version" : 2,
  "permissions" : [
    "tabs", "*://github.com/*",
    "*://travis-ci.org/*", "*://travis-ci.com/*"
  ],
  "browser_action": {
     "default_icon": "icon.png",
     "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["*://github.com/*"],
      "js": ["jquery.js", "travisreport.js"]
    }
  ]
}